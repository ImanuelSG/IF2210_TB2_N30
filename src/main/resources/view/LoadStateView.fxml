<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="loadState" alignment="TOP_CENTER" prefHeight="552.0" prefWidth="1000" style="-fx-background-radius: 20px; -fx-background-color: white; -fx-padding: 30;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoadStateController">
   <children>
      <Label text="Load State">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" />
      <Button text="Select File" onAction="#handleSelectFileButtonAction" />
   </children>
</VBox>
